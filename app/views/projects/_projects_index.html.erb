 <li class="tab_list" id="project_<%= project.id.to_s %>"> 
  <div class="tab_list_text">
 <%= link_to project.name, project_path(project) %>
</div>
  <span class='view-tabs-list' data-id="<%= project.id %>" data-object="project" title="View project" data-firm-id="<%= current_firm.id %>"></span>
 
  <%= link_to(project_path(project), :confirm => "Are you sure?", :title => "Delete project", :method => :delete, :remote => true) do%>
  <span class='delete-tabs-list'></span>
    <% end %>

  <span class='open_project_update button edit' id="project_update" data-id="<%= project.id %>" data-object="project" title="Edit project">Edit</span>  
 <a class="activate_project button" data-object="project" data-id="<%= project.id.to_s %>">Archive</a>
 <div class="dialog_form" id="project_update_<%= project.id %>_form" title="Update project" style="display:none;"></div>
</li>